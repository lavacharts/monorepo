{
  "name": "@lavacharts/core",
  "version": "4.0.0-rc1",
  "private": true,
  "description": "Wrapper library for the Google Chart API",
  "keywords": [
    "google",
    "charts",
    "graphs",
    "lavacharts",
    "javascript",
    "typescript"
  ],
  "homepage": "https://lavacharts.github.io/lava.js/",
  "bugs": {
    "url": "https://github.com/lavacharts/lava.js/issues"
  },
  "repository": "git+https://github.com/lavacharts/lava.js.git",
  "license": "MIT",
  "author": "Kevin Hill",
  "main": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dist": "tsup && pnpm types",
    "types": "pnpm clean:dts && tsc --emitDeclarationOnly --declaration",
